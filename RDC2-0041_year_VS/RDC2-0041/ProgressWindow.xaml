<Window x:Class="RDC2_0041.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RDC2_0041"
        mc:Ignorable="d"
        Title="ProgressWindow" Height="90" Width="330" ResizeMode="NoResize" WindowStyle="None" WindowStartupLocation="CenterOwner">
    <Window.Background>
        <ImageBrush ImageSource="images/back.jpg"/>
    </Window.Background>
    <Grid>
        <ProgressBar HorizontalAlignment="Center" Height="30" Margin="10,30,10,0" VerticalAlignment="Top" Width="280" IsIndeterminate="True" BorderThickness="0"/>

    </Grid>
</Window>
